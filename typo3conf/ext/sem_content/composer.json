{
    "name": "media711/sem-content",
    "type": "typo3-cms-extension",
    "description": "Basic content configuration for TYPO3 CMS projects",
    "version": "2.0.6",
    "homepage": "https://www.711media.de/",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS", "content", "distribution", "fluid_styled_content", "fsc"],
    "support": {
        "issues": "https://git.711media.de/TYPO3/sem_content/issues"
    },
    "require": {
        "typo3/cms-core": "*",
        "typo3/cms-fluid-styled-content": "*",
        "typo3-ter/gridelements": "*",
        "media711/sem-sitepackage": "*"
    },
    "autoload": {
        "psr-4": {
            "Media711\\SemContent\\": "Classes/"
        }
    },
    "replace": {
        "sem_content": "self.version",
        "typo3-ter/sem-content": "self.version"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.typo3.org"
        },
        {
            "type": "vcs",
            "url": "https://git.711media.de/TYPO3/sem_sitepackage"
        }
    ]
}
